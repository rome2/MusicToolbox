<resources>
    <declare-styleable name="PianoKeys">
        <attr name="keySizeRatio" format="float" />
        <attr name="drawKeyDivider" format="boolean" />
        <attr name="smallKeysVisible" format="boolean" />
        <attr name="drawSmallKeyOutline" format="boolean" />
        <attr name="drawBigKeyOutline" format="boolean" />
        <attr name="markMiddleC" format="boolean" />
        <attr name="markAllCs" format="boolean" />
        <attr name="lineColor" format="color|reference" />
        <attr name="lineShadeColor" format="color|reference" />
        <attr name="whiteKeyColor" format="color|reference" />
        <attr name="whiteKeyShadeColor" format="color|reference" />
        <attr name="blackKeyColor" format="color|reference" />
        <attr name="blackKeyShadeColor" format="color|reference" />
        <attr name="keyDownKeyColor" format="color|reference" />
        <attr name="keyDownKeyShadeColor" format="color|reference" />
    </declare-styleable>
</resources>
